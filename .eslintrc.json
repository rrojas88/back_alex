{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "quotes": "off",
        "arrow-parens": "off",
        "no-console": "off",
        "comma-dangle": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off",
        "camelcase": "off",
        "spaced-comment": "off",
        "array-bracket-spacing": "off",
        "space-in-parens": "off",
        "no-trailing-spaces": "off"
    }
}